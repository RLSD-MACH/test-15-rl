<odoo>
    <record id="l10n_dk" model="mis.report.style">
        <field name="name">l10n_dk</field>
        <field name="dp_inherit" eval="0"/>
        <field name="dp">2</field>
    </record>
    <record id="l10n_dk_caption" model="mis.report.style">
        <field name="name">l10n_dk overskrift</field>
        <field name="font_weight_inherit" eval="0"/>
        <field name="font_weight">bold</field>
        <field name="font_size_inherit" eval="0"/>
        <field name="font_size">large</field>
    </record>
    <record id="l10n_dk_total" model="mis.report.style">
        <field name="name">l10n_dk total</field>
        <field name="font_weight_inherit" eval="0"/>
        <field name="font_weight">bold</field>
    </record>
     <record id="l10n_dk_sum" model="mis.report.style">
        <field name="name">l10n_dk sum</field>
        <field name="font_weight_inherit" eval="0"/>
        <field name="font_weight">bold</field>
        <field name="color_inherit" eval="0"/>
        <field name="color">#0C0C0C</field>
        <field name="background_color_inherit" eval="0"/>
        <field name="background_color">#E1E1E6</field>
    </record>
    <record id="l10n_dk_sub" model="mis.report.style">
        <field name="name">l10n_dk sub</field>
        <field name="font_weight_inherit" eval="0"/>
    </record>
   <record id="l10n_dk_detail" model="mis.report.style">
        <field name="name">l10n_dk detaljer</field>
        <field name="font_style_inherit" eval="0"/>
        <field name="font_style">italic</field>
        <field name="color_inherit" eval="0"/>
        <field name="color">#00a09d</field>
        <field name="indent_level_inherit" eval="0"/>
        <field name="indent_level">1</field>
    </record>
</odoo>
