<?xml version="1.0" encoding="utf-8"?>
<odoo>
    
    <record id="view_stock_quantity_history_ext_form" model="ir.ui.view">

        <field name="name">view_stock_quantity_history_ext.form</field>
        <field name="model">stock.quantity.history</field>
        <field name="inherit_id" ref="stock.view_stock_quantity_history"/>      
        <field name="arch" type="xml">

            <xpath expr="//field[@name='inventory_datetime']" position="after">

                <field name="owner_id" options="{'no_create': True, 'no_open': False}" />
            
            </xpath>

        </field>

    </record>  
</odoo>
