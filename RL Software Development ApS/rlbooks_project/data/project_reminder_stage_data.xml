<?xml version="1.0" encoding="utf-8"?>

<odoo>

    <data noupdate="1">

        <record model="rlbooks.project.reminder.stage">            
            <field name="name">New</field>
            <field name="active">True</field>
            <field name="is_closed">False</field>
            <field name="sequence">0</field>
            <field name="company_id" ref="base.main_company"/>
        </record>

        <record model="rlbooks.project.reminder.stage">            
            <field name="name">Awaiting process</field>
            <field name="active">True</field>
            <field name="is_closed">False</field>
            <field name="sequence">1</field>
            <field name="company_id" ref="base.main_company"/>
        </record>

        <record model="rlbooks.project.reminder.stage">            
            <field name="name">Commenced</field>
            <field name="active">True</field>
            <field name="is_closed">False</field>
            <field name="sequence">2</field>
            <field name="company_id" ref="base.main_company"/>
        </record>

        <record model="rlbooks.project.reminder.stage">            
            <field name="name">Awaiting signature</field>
            <field name="active">True</field>
            <field name="is_closed">False</field>
            <field name="sequence">3</field>
            <field name="company_id" ref="base.main_company"/>
        </record>

        <record model="rlbooks.project.reminder.stage">            
            <field name="name">Closed</field>
            <field name="active">True</field>
            <field name="is_closed">True</field>
            <field name="sequence">4</field>
            <field name="company_id" ref="base.main_company"/>
        </record>

    </data>

</odoo>
